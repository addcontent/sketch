# This file contains environment variables for running the sketch application.
# Copy this file to .env and fill in your API keys.

# Azure OpenAI Service
# Used when running with any Azure model (e.g., '--model azure-gpt4o')
AZURE_OPENAI_KEY="your_azure_openai_api_key"
AZURE_OPENAI_ENDPOINT="https://your-azure-openai-resource.openai.azure.com/"

# Google Gemini
# Used when running with '--model gemini'
GEMINI_API_KEY="your_google_gemini_api_key"

# OpenAI
# Used for models like gpt4.1, gpt4o, etc.
OPENAI_API_KEY="your_openai_api_key"

# Anthropic Claude (Default Model)
# Used when running with '--model claude' or no model flag
ANTHROPIC_API_KEY="your_anthropic_api_key"

# Other Supported Services
FIREWORKS_API_KEY="your_fireworks_api_key"
TOGETHER_API_KEY="your_together_api_key"
MISTRAL_API_KEY="your_mistral_api_key"
